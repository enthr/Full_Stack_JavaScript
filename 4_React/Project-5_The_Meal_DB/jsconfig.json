{
    "compilerOptions": {
        "baseUrl": ".",
        "paths": {
            "@assets/*": [
                "src/assets/*"
            ],
            "@features/*": [
                "src/features/*"
            ],
            "@layouts/*": [
                "src/layouts/*"
            ],
            "@lib/*": [
                "src/lib/*"
            ],
            "@pages/*": [
